{
  "version": 2,
  "framework": null,
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@20"
    }
  },
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node@20",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
